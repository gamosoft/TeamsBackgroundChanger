Windows Registry Editor Version 5.00

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\Kind.Picture\shell\assign-teams-background]
@="Assign as Teams Background"
"Icon"=" %USERPROFILE%\\AppData\\Local\\Microsoft\\Teams\\app.ico"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\Kind.Picture\shell\assign-teams-background\command]
@="PowerShell.exe -NoLogo -NoProfile -WindowStyle Hidden -Command \"Copy-Item '%1' $env:APPDATA\\Microsoft\\Teams\\Backgrounds\\Uploads\\background-changer.png; Copy-Item '%1' $env:APPDATA\\Microsoft\\Teams\\Backgrounds\\Uploads\\background-changer_thumb.png;\""
